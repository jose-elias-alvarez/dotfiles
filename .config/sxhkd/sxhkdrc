super + Return
	alacritty
super + x
	firefox

super + @space
	rofi -show drun
alt + Tab
	rofi -show window
super + alt + c
	CM_LAUNCHER=rofi clipmenu

super + Escape
	pkill -USR1 -x sxhkd
super + alt + {q,r}
	bspc {quit,wm -r}

super + {_,shift + }q
	bspc node -{c,k} super + {_,shift + }{h,j,k,l}
	bspc node -{f,s} {west,south,north,east}

super + m
	bspc desktop -l next
super + bracket{left,right}
	bspc desktop -f {prev,next}.local

super + {1-9}
	bspc {desktop -f} ^{1-9}
super + shift + {1-9}
    id=$(bspc query -N -n); bspc node -d ^{1-9}; bspc node -f ${id}
